{
	"name": "news-studio-thing",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerStorage": {
			"TS": {
				"$path": "out/serverStorage"
			},
			"Freecam": {
			  "$path": "models/Freecam.rbxmx"
			},
			"SpotlightBrightness": {
			  "$className": "NumberValue",
			  "$properties": {
				"Value": 2.72
			  }
			},
			"IsSpotlightOn": {
			  "$className": "BoolValue",
			  "$properties": {
				"Value": true
			  }
			},
			"RoofLights": {
			  "$className": "Folder",
			  "Brightness": {
				"$className": "NumberValue",
				"$properties": {
				  "Value": 4
				}
			  },
			  "Colour": {
				"$className": "Color3Value",
				"$properties": {
				  "Value": [
					1,
					1,
					1
				  ]
				}
			  },
			  "Transparency": {
				"$className": "NumberValue",
				"$properties": {
				  "Value": 0
				}
			  }
			},
			"IsScreenOn": {
			  "$className": "BoolValue",
			  "$properties": {
				"Value": true
			  }
			},
			"ScreenColour": {
			  "$className": "Color3Value",
			  "$properties": {
				"Value": [
				  0,
				  0,
				  0
				]
			  }
			}
		  },
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			},
			"StartCountdown": {
			  "$className": "RemoteEvent"
			},
			"SetScreenColour": {
			  "$className": "RemoteEvent"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
			"CountdownSound": {
				"$className": "Sound",
				"$properties": {
				  "SoundId": "rbxassetid://3735519646"
				}
			  },
			  "Outside": {
				"$path": "models/Outside.rbxmx"
			  },
			  "Studio": {
				"$path": "models/StudioStructure.rbxmx",
				"Screen": {
				  "$path": "models/Screen.rbxmx"
				},
				"StageLights": {
				  "$path": "models/StageLights.rbxmx"
				},
				"LightControls": {
				  "$path": "models/LightControls.rbxmx"
				},
				"ScreenControls": {
				  "$className": "Folder",
				  "ToggleScreen": {
					"$path": "models/ToggleScreen.rbxmx"
				  },
				  "ScreenColours": {
					"$path": "models/ScreenColours.rbxmx"
				  },
				  "Countdown": {
					"$path": "models/Countdown.rbxmx"
				  }
				},
				"RoofLights": {
				  "$path": "models/RoofLights.rbxmx"
				},
				"RoofLightControls": {
				  "$path": "models/RoofLightControls.rbxmx"
				}
			  }
			},
			"Lighting": {
			  "$properties": {
				"Ambient": [
				  0,
				  0,
				  0
				],
				"Brightness": 2,
				"GlobalShadows": true,
				"Outlines": false,
				"Technology": "Future"
			  },
			  "Sky": {
				"$path": "models/Sky.rbxmx"
			  }
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
